{
  "expo": {
    "name": "토크토크",
    "slug": "talktalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#100f13"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ksnd0297.talktalk"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#100f13"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ksnd0297.talktalk"
    },
    "extra": {
      "eas": {
        "projectId": "6c03b3e3-ffed-4245-9da8-63c44fd14aee"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#100f13",
          "image": "./assets/splash-icon.png"
        }
      ]
    ]
  }
}
